{
  "entryFile": "app.ts",
  "routes": {
    "routesDir": "generated",
    "authenticationModule": "./services/Authentication.ts",
    "esm": true
  },
  "controllerPathGlobs": ["controllers/*Controller.ts"],
  "spec": {
    "outputDirectory": "generated",
    "specVersion": 3,
    "securityDefinitions": {
      "jwt": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      }
    },
    "security": [
      {
        "jwt": []
      }
    ]
  }
}